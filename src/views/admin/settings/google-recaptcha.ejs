<div class="aiz-content-wrapper">
  <%- include('../partials/topbar') %>
    <!-- .aiz-topbar -->
  <div class="aiz-main-content">
    <div class="px-15px px-lg-25px">
      <div class="row">
        <div class="col-md-6">
          <div class="card">
            <div class="card-header">
              <h3 class="mb-0 h6">Google reCAPTCHA Setting</h3>
            </div>
            <div class="card-body">
              <form class="form-horizontal" action="" method="POST">
                <div class="form-group row">
                  <div class="col-md-4">
                    <label class="control-label">Google reCAPTCHA</label>
                  </div>
                  <div class="col-md-8">
                    <label class="aiz-switch aiz-switch-success mb-0">
                      <input value="1" name="google_recaptcha" type="checkbox" checked="">
                      <span class="slider round"></span>
                    </label>
                  </div>
                </div>
                <div class="form-group row">
                  <input type="hidden" name="types[]" value="CAPTCHA_KEY">
                  <div class="col-md-4">
                    <label class="control-label">Site KEY</label>
                  </div>
                  <div class="col-md-8">
                    <input type="text" class="form-control" name="CAPTCHA_KEY" value="" placeholder="Site KEY" required="">
                  </div>
                </div>
                <div class="form-group row">
                  <input type="hidden" name="types[]" value="RECAPTCHA_SECRET_KEY">
                  <div class="col-md-4">
                    <label class="control-label">SECRET KEY</label>
                  </div>
                  <div class="col-md-8">
                    <input type="text" class="form-control" name="RECAPTCHA_SECRET_KEY" value="" placeholder="SECRET KEY" required="">
                  </div>
                </div>
                <div class="form-group mb-0 text-right">
                  <button type="submit" class="btn btn-sm btn-primary">Save</button>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
    <%- include('../partials/version.ejs') %>
  </div><!-- .aiz-main-content -->
</div>