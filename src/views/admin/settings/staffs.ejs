<div class="aiz-content-wrapper bg-white">
  <%- include('../partials/topbar.ejs') %>
    <!-- .aiz-topbar -->
  <div class="aiz-main-content">
    <div class="px-15px px-lg-25px">

      <div class="aiz-titlebar text-left mt-2 mb-3">
        <div class="row align-items-center">
          <div class="col-md-6">
            <h1 class="h3">All staffs</h1>
          </div>
          <div class="col-md-6 text-md-right">
            <a href="/admin/staffs/create" class="btn btn-circle btn-info">
              <span>Add New Staffs</span>
            </a>
          </div>
        </div>
      </div>

      <div class="card">
        <div class="card-header">
          <h5 class="mb-0 h6">Staffs</h5>
        </div>
        <div class="card-body">
          <table class="table aiz-table mb-0 footable footable-1 breakpoint breakpoint-lg" style="">
            <thead>
              <tr class="footable-header">





                <th data-breakpoints="lg" width="10%" style="display: none;">#</th>
                <th class="footable-first-visible" style="display: table-cell;">Name</th>
                <th data-breakpoints="lg" style="display: none;">Email</th>
                <th data-breakpoints="lg" style="display: none;">Phone</th>
                <th data-breakpoints="lg" style="display: none;">Role</th>
                <th width="10%" class="text-right footable-last-visible" style="display: table-cell;"></th>
              </tr>
            </thead>
            <tbody>
              <% allstaffs.forEach(staff => { %>
                <tr>





                  <td style="display: none;"></td>
                  <td class="footable-first-visible" style="display: table-cell;"><span class="footable-toggle fooicon fooicon-plus"></span><%= staff.name %></td>
                  <td style="display: none;"><%= staff.email %></td>
                  <td style="display: none;"><%= staff.phone %></td>
                  <td style="display: none;">
                    <%= staff.role %>
                  </td>
                  <td class="text-right footable-last-visible" style="display: table-cell;">
                    
                  </td>
                </tr>
              <% }) %>
              
              
            </tbody>
          </table>
          <div class="aiz-pagination">

          </div>
        </div>
      </div>

    </div>
    <div class="bg-white text-center py-3 px-15px px-lg-25px mt-auto border-top">
      <p class="mb-0">Â© v1.0</p>
    </div>
  </div><!-- .aiz-main-content -->
</div>